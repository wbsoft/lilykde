#: ../lilykde/lqi.py:304
msgid "Rhythm"
msgstr "Ritme"

#: ../lilykde/lqi.py:306
msgid "Different tools to edit durations."
msgstr "Hulpmiddelen voor het bewerken van tijdsduren."

#: ../lilykde/lqi.py:311
msgid "Durations"
msgstr "Tijdsduren"

#: ../lilykde/lqi.py:315
msgid "Double durations"
msgstr "Tijdsduur verdubbelen"

#: ../lilykde/lqi.py:316
msgid "Double all the durations in the selection."
msgstr "Alle nootlengtes in de selectie verdubbelen."

#: ../lilykde/lqi.py:317
msgid "Halve durations"
msgstr "Tijdsduur halveren"

#: ../lilykde/lqi.py:318
